# mysql
fastcgi_param  MYSQL_HOST_R  host.docker.internal;
fastcgi_param  MYSQL_PORT_R  9501;
fastcgi_param  MYSQL_USER_R  root;
fastcgi_param  MYSQL_PASS_R  123456;
fastcgi_param  MYSQL_NAME_R  cu_rbac;

fastcgi_param  MYSQL_HOST_W  host.docker.internal;
fastcgi_param  MYSQL_PORT_W  9501;
fastcgi_param  MYSQL_USER_W  root;
fastcgi_param  MYSQL_PASS_W  123456;
fastcgi_param  MYSQL_NAME_W  cu_rbac;

# mongo
